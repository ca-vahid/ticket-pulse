# Node Environment
NODE_ENV=development

# Server Configuration
PORT=3000

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/ticketpulse

# Session Security
SESSION_SECRET=your-super-secret-session-key-change-this

# FreshService API Configuration
FRESHSERVICE_API_KEY=your-freshservice-api-key-here
FRESHSERVICE_DOMAIN=your-domain.freshservice.com
FRESHSERVICE_WORKSPACE_ID=your-workspace-id

# Authentication
ADMIN_PASSWORD_HASH=$2b$10$your-bcrypt-hashed-password-here

# Azure Configuration (Production Only)
# AZURE_KEY_VAULT_URL=https://your-vault.vault.azure.net/
# APPLICATION_INSIGHTS_KEY=your-app-insights-key

# Logging
LOG_LEVEL=info
